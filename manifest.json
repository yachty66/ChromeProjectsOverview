{
  "name": "ChromeProjectsOverview",
  "description": "Displays all of my current projects in a new tab in GChrome",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs"],
  "action": {
    "default_icon": {
      "128": "/images/white.png"
    }
  },
  "icons": {
    "128": "/images/white.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
